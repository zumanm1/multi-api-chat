# API Keys for different providers
# Copy this file to .env and fill in your actual API keys

OPENAI_API_KEY=
GROQ_API_KEY=
CEREBRAS_API_KEY=
SAMBANOVA_API_KEY=
OPENROUTER_API_KEY=
ANTHROPIC_API_KEY=

# Flask configuration
FLASK_ENV=development
FLASK_DEBUG=true

# AI Agent Configuration (optional - only needed if using AI features)
# Set to false to completely disable AI agents
ENABLE_AI_AGENTS=true

# AI agent debugging (set to true for detailed AI agent logs)
AI_AGENTS_DEBUG=false

# Default LLM for AI agents (options: gpt-4, gpt-3.5-turbo, ollama models, etc.)
AI_AGENTS_DEFAULT_LLM=gpt-4

# Maximum number of concurrent AI agent tasks
AI_AGENTS_MAX_CONCURRENT=3
